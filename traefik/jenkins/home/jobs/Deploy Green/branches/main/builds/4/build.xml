    <script>pipeline {
  environment {
    imagename = &quot;apisample&quot;
    dockerImage = &apos;&apos;
  }
  agent any
  stages {
    stage(&apos;Building image&apos;) {
      steps{
        script {
          dockerImage = docker.build imagename
        }
      }
    }
    stage(&apos;Deploy Image&apos;) {
      steps{
        script {
          docker.withRegistry( &apos;https://myregistry.local:5000&apos; ) {
            dockerImage.push(&quot;$BUILD_NUMBER&quot;)
             dockerImage.push(&apos;green&apos;)

          }
        }
      }
    }
    stage(&apos;Launch blue&apos;) {
      steps{
	sh &quot;docker stack deploy -c jenkins/appli-blue.yml green&quot;
      }
    }
  }
}
</script>
